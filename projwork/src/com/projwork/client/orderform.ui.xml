<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	
xmlns:g="urn:import:com.google.gwt.user.client.ui"
	 
xmlns:d="urn:import:gwt.material.design.client.ui">
	
	
<g:HTMLPanel>
	
<h2> Order Form</h2>
	
	
<d:MaterialTextBox text="Name" ui:field="t1" iconType="ACCOUNT_CIRCLE">
</d:MaterialTextBox>
	
<d:MaterialTextBox text="Address" ui:field="t2" iconType="MY_LOCATION">
</d:MaterialTextBox>
	
<d:MaterialTextBox label="City" ui:field="t3" iconType="MAP">
</d:MaterialTextBox>
	
<d:MaterialTextBox label="State" ui:field="t4" iconType="MAP">
</d:MaterialTextBox>

<d:MaterialTextBox label="Phone1" ui:field="t5" iconType="PHONE">
</d:MaterialTextBox>
	
<d:MaterialTextBox label="Phone2" ui:field="t6" iconType="PHONE">
</d:MaterialTextBox>
	
<d:MaterialTextBox  label="Email" ui:field="t7" iconType="LOCAL_POST_OFFICE">
</d:MaterialTextBox>
	
<d:MaterialButton text="Confirm Request"  ui:field="t8" waves="LIGHT" textColor="WHITE">
</d:MaterialButton>
	
	

	
</g:HTMLPanel>

</ui:UiBinder> 