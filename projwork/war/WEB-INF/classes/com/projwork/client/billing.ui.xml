<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:gwt.material.design.client.ui">
	
	
	<g:HTMLPanel>
		<h2> Billing </h2>
	
	<d:MaterialTextBox label="Order Id" ui:field="t1" iconType="ACCOUNT_CIRCLE"></d:MaterialTextBox>
	<d:MaterialTextBox label="Client Name" ui:field="t2" iconType="MY_LOCATION"></d:MaterialTextBox>
	<d:MaterialTextBox label="Address" ui:field="t3" iconType="MAP"></d:MaterialTextBox>
	<d:MaterialTextBox label="Phone No." ui:field="t4" iconType="PHONE"></d:MaterialTextBox>
	<d:MaterialTextBox label="Order Date" ui:field="t5" iconType="LOCAL_POST_OFFICE"></d:MaterialTextBox>
	
		
	<d:MaterialTextBox  label="Market Price" ui:field="t7" iconType="LOCAL_POST_OFFICE"></d:MaterialTextBox>
	
	<d:MaterialTextBox label="Discount" ui:field="t6" iconType="LOCAL_POST_OFFICE"></d:MaterialTextBox>
	
	<d:MaterialTextBox  label="Total Amount" ui:field="t9" iconType="LOCAL_POST_OFFICE"></d:MaterialTextBox>
	<d:MaterialButton text="Proceed"  ui:field="b1" waves="LIGHT" textColor="WHITE"></d:MaterialButton>
	
		
	</g:HTMLPanel>
</ui:UiBinder> 